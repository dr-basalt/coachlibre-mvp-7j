# =================================
# COACHLIBRE MVP - DEVELOPMENT ENV
# =================================

# Cluster Configuration
ENVIRONMENT=development
CLUSTER_NAME=coachlibre-dev
DOMAIN=dev.coachlibre.local

# Docker Registry
DOCKER_REGISTRY=docker.io
DOCKER_NAMESPACE=coachlibre

# Frontend Configuration
ASTRO_PORT=3000
ASTRO_HOST=0.0.0.0
PAYLOAD_API_URL=http://payload-backend:3001
TINA_CLIENT_ID=your_tina_client_id
TINA_TOKEN=your_tina_token

# Backend Configuration
PAYLOAD_PORT=3001
PAYLOAD_SECRET=your-dev-payload-secret-key
DATABASE_URL=postgresql://postgres:changeme@postgres:5432/coachlibre
SMTP_HOST=mailhog
SMTP_PORT=1025
SMTP_USER=
SMTP_PASS=

# AI Services
FLOWISE_PORT=3002
FLOWISE_DATABASE_PATH=/opt/flowise/.flowise
OLLAMA_API_URL=http://ollama:11434
OPENAI_API_KEY=your_openai_key

# Workflow Services  
N8N_PORT=5678
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme
N8N_WEBHOOK_URL=https://workflows.dev.coachlibre.local

# Vector Database
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_key

# Video Services
LIVEKIT_PORT=7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=devsecret

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=coachlibre

# Redis
REDIS_URL=redis://redis:6379
